.preventDefault();\r\n    setIsLoading(true);\r\n    const enteredInformations = ResponseData.current.value;\r\n\r\n    const result = await sendMessage(\r\n      enteredInformations,\r\n      ItemTitle,\r\n      PostID,\r\n      PosterID\r\n    );\r\n    event.target.reset();\r\n    setIsLoading(false);\r\n  }\r\n\r\n  if (isLoading) {\r\n    return <div style={{ fontSize: \"1.5rem\" }}>Submiting...</div>;\r\n  }\r\n  return (\r\n    <Fragment>\r\n      <form className={classes.form} onSubmit={InfosubmitHandler}>\r\n        <label for=\"contactInformations\">Enter your Contact Informations</label>\r\n        <textarea\r\n          type=\"text\"\r\n          name=\"contactInformations\"\r\n          rows=\"4\"\r\n          cols=\"40\"\r\n          placeholder=\"Enter your Gmail / Cellno\"\r\n          ref={ResponseData}\r\n          required\r\n        ></textarea>\r\n\r\n        <div>\r\n          <button className={classes.button89} type=\"submit\" role=\"button\">\r\n            Submit\r\n          </button>\r\n          <button\r\n            className={classes.button89}\r\n            type=\"button\"\r\n            role=\"button\"\r\n            onClick={props.close}\r\n          >\r\n            Close\r\n          </button>\r\n        </div>\r\n      </form>\r\n    </Fragment>\r\n  );\r\n};\r\n\r\nexport default DetailForm;\r\n"]},"metadata":{},"sourceType":"module"}